{
  "name": "@global-modules/runtime",
  "version": "0.0.0",
  "packageManager": "yarn@4.5.0",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "./dist/**/*.js",
    "./cjs/**/*.cjs",
    "./esm/**/*.mjs"
  ],
  "scripts": {
    "prepack": "yarn build",
    "start": "node --import @swc-node/register/esm-register src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "cleanup": "rm -rf dist cjs esm",
    "build": "yarn cleanup && node build/index.mjs && tsc -p tsconfig.build.json"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.9",
    "@swc/core": "1.7.x",
    "@swc/helpers": "^0.5.13",
    "@swc/types": "^0.1.12",
    "@types/node": "^20",
    "esbuild": "^0.24.0",
    "esbuild-dependency-graph": "^0.13.0",
    "typescript": "5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.1",
    "zx": "^8.1.8"
  }
}
